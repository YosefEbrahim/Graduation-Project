@model DoctorViewModel
@{
    ViewData["Title"] = "Create";
}


<form asp-action="Create" asp-controller="Doctors" asp-area="Admin" method="post" enctype="multipart/form-data">
    <div class="form-group">
        <label asp-for="Email" class="control-label"></label>
        <input asp-for="Email" class="form-control" type="text" />
        <span asp-validation-for="Email" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Name" class="control-label"></label>
        <input asp-for="Name" class="form-control" type="text" />
        <span asp-validation-for="Name" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="PhoneNumber" class="control-label"></label>
        <input asp-for="PhoneNumber" class="form-control" type="text" />
        <span asp-validation-for="PhoneNumber" class="text-danger"></span>
    </div>
    <div class="form-group">
        <label asp-for="Specialice" class="control-label"></label>
        <input asp-for="Specialice" class="form-control" type="text" />
        <span asp-validation-for="Specialice" class="text-danger"></span>
    </div>
            <div class="form-group">
     <label asp-for="Departments" class="control-label"></label>
     <select  asp-items="@Model.Departments" name="DeptId" class="customSelect">
     </select>
     </div>
   <div class="form-group">
     <label asp-for="Subjects" class="control-label"></label>
     <select  asp-items="@Model.Subjects" name="SubjectsId" class="customSelect select" multiple>
     </select>
     </div>
    <div class="form-group">
        <label asp-for="Password" class="control-label"></label>
        <input asp-for="Password" class="form-control" type="password" />
        <span asp-validation-for="Password" class="text-danger"></span>
    </div>
    <a class="btn btn-info" asp-action="Index" asp-controller="Doctors">Back</a>
    <button type="submit" class="btn btn-success">Add</button>
</form>
